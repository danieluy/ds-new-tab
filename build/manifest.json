{
  "manifest_version": 2,
  "name": "DSNewTab",
  "description": "Chrome's new tab replacement by Daniel Sosa",
  "version": "0.1.0",
  "icons": {
    "16" : "icons/16x16.png",
    "64" : "icons/64x64.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["reset.css", "styles.css", "roboto.css"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "storage",
    "bookmarks"
  ]
}
